<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vbx_FilterBackground" prefHeight="990.0" prefWidth="340.0" spacing="20.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.filmverleih.filmverleih.FilterController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="49.0" prefWidth="340.0" text="Testfilter">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <TextField fx:id="testFilter" />
      <Button fx:id="applyButton" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#generateFilters" prefHeight="25.0" prefWidth="368.0" text="Apply Filters" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
